{
    "namespace": "TruJS.cmdArgs"
    , "entries": [
        {
            "type": "module"
            , "name": "trujs-cmdargs"
            , "description": "A simple module for parsing command line arguments in NodeJS"
            , "repos": [
                {
                    "type": "git"
                    , "url": "https://github.com/trujs/TruJS.core.git"
                    , "branch": "development"
                    , "local": "TruJS/core"
                }
            ]
            , "paths": [
                "{TruJS.core}/utils/_RegEx.js"
                , "{TruJS.cmdArgs}/_CmdArgs.js"
            ]
            , "moduleExport": "cmdArgs"
            , "config": {
                "strict": true
                , "fileName": "cmdArgs.js"
            }
            , "module": {
                "statements": {
                    "beginning": [
                        "var args = process.argv;"
                    ]
                }
            }
        }
    ]
}